<PropertyList>

  <path>plaque_restriction.ac</path>

  <animation>
    <type>material</type>
    <object-name>restrictions</object-name>
    <condition>
      <not><property>sim/rendering/rembrandt/enabled</property></not>
    </condition>
    <emission>
      <red>   0.003 </red>
      <green> 0.003 </green>
      <blue>  0.003 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

</PropertyList>
