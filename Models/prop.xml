<?xml version="1.0"?>
<PropertyList>

  <path>prop.ac</path>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>propeller-running</object-name>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
  </effect>

  <animation>
    <type>select</type>
    <object-name>propeller-running</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>250</value>
      </greater-than>
    </condition>
  </animation>

</PropertyList>
