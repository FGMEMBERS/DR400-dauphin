      <y-m>  1.4045 </y-m>
      <z-m> -1.1026 </z-m>
    </offsets>
  </model>


  <!-- Transparent objects -->
  <effect>
    <inherits-from>Aircraft/DR400-dauphin/Models/Effects/effects-glasses</inherits-from>
    <object-name>rear-windows</object-name>
    <object-name>front-windows</object-name>
    <object-name>landing-light-windows</object-name>
  </effect>

  <effect>
    <inherits-from>Aircraft/DR400-dauphin/Models/Effects/effects-glasses-interior</inherits-from>
    <object-name>rear-windows-int</object-name>
    <object-name>front-windows-int</object-name>
  </effect>

  <!-- Propeller -->
  <animation>
    <type>spin</type>
    <object-name>propeller</object-name>
    <object-name>cone-propeller</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -3.1638 </x-m>
      <y-m>  0.00135 </y-m>
      <z-m>  0.08872 </z-m>
    </center>
    <axis>
      <x> -1.000 </x>
      <y>  0.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propeller</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>250</value>
      </less-than>
    </condition>
  </animation>

  <model>
    <path>Aircraft/DR400-dauphin/Models/prop.xml</path>
    <offsets>
      <x-m> -3.1638 </x-m>
      <y-m>  0.00135 </y-m>
      <z-m>  0.08872 </z-m>
    </offsets>
  </model>

  <!-- Elevator -->
  <animation>
    <type>rotate</type>
    <object-name>elevator</object-name>
    <object-name>trim-elevator</object-name>
    <property>controls/flight/elevator</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -12.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>   9.5 </dep></entry>
    </interpolation>
    <center>
      <x-m> 2.2808 </x-m>
      <y-m> 0.0000 </y-m>
      <z-m> 0.2837 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Elevator-trim -->
  <animation>
    <type>rotate</type>
    <object-name>trim-elevator</object-name>
    <property>controls/flight/elevator-trim</property>
    <factor>20</factor>
    <center>
      <x-m> 2.9021 </x-m>
      <y-m> 0.0000 </y-m>
      <z-m> 0.2433 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Rudder -->
  <animation>
    <type>rotate</type>
    <object-name>rudder</object-name>
    <property>controls/flight/rudder</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -25.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  25.0 </dep></entry>
    </interpolation>
    <axis>
      <x1-m>  2.8180 </x1-m>
      <y1-m>  0.0001 </y1-m>
      <z1-m> -0.1460 </z1-m>
      <x2-m>  3.0009 </x2-m>
      <y2-m>  0.0001 </y2-m>
      <z2-m>  1.3365 </z2-m>
    </axis>
  </animation>

  <!-- Ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronL</object-name>
    <property>controls/flight/aileron</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep>  15.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep> -10.0 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -0.29240 </x1-m>
      <y1-m> -2.78200 </y1-m>
      <z1-m> -0.35350 </z1-m>
      <x2-m> -0.92120 </x2-m>
      <y2-m> -4.38470 </y2-m>
