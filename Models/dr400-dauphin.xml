      <y2-m>  4.38590 </y2-m>
      <z2-m>  0.06662 </z2-m>
    </axis>
  </animation>

  <!-- Flaps -->
  <animation>
    <type>rotate</type>
    <object-name>flapsL</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>60.0</factor>
    <axis>
      <x1-m> -0.2204 </x1-m>
      <y1-m> -2.7820 </y1-m>
      <z1-m> -0.3580 </z1-m>
      <x2-m> -0.2171 </x2-m>
      <y2-m> -0.6544 </y2-m>
      <z2-m> -0.3792 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>flapsR</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>60.0</factor>
    <axis>
      <x1-m> -0.2171 </x1-m>
      <y1-m>  0.6555 </y1-m>
      <z1-m> -0.3792 </z1-m>
      <x2-m> -0.2204 </x2-m>
