    <condition>
      <not><property>sim/multiplay/generic/int[14]</property></not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>left-absorber</object-name>
    <object-name>left-wheel</object-name>
    <object-name>gearL</object-name>
    <condition>
      <not><property>sim/multiplay/generic/int[15]</property></not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>right-absorber</object-name>
    <object-name>right-wheel</object-name>
    <object-name>gearR</object-name>
    <condition>
      <not><property>sim/multiplay/generic/int[16]</property></not>
    </condition>
  </animation>

  <!-- Tutorial marker -->
  <model>
    <path>Models/Aircraft/marker.xml</path>
  </model>

  <!-- Pilot -->
  <model>
    <path>Aircraft/DR400-dauphin/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -1.50 </x-m>
      <y-m> -0.25 </y-m>
      <z-m>  0.0 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   0.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>

  <!-- Interieur -->
  <model>
    <path>Aircraft/DR400-dauphin/Models/Interior/interior.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.0 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   0.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>

  <!-- Water effects -->
  <model>
    <path>Aircraft/DR400-dauphin/Models/Effects/Water/front-wheel.xml</path>
    <offsets>
      <x-m> -2.5639 </x-m>
      <y-m> -0.0538 </y-m>
      <z-m> -1.1137 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/DR400-dauphin/Models/Effects/Water/left-wheel.xml</path>
